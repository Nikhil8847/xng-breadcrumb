{
  "name": "xng-breadcrumb-workspace",
  "version": "10.0.2",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build xng-breadcrumb --configuration production",
    "build:default": "nx build default --configuration production",
    "test": "nx test",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:lint": "nx affected:lint",
    "copy-readme": "cp README.md libs/xng-breadcrumb/README.md",
    "copy-version": "VERSION=$npm_package_version && cd libs/xng-breadcrumb && npm version $VERSION && cd ../..",
    "commit-version": "git add . && git commit -m $npm_package_version && git push origin main",
    "prepackage": "git checkout main && git pull origin main && npm-run-all copy-* commit-version",
    "package": "pnpm build && npm publish ./dist/libs/xng-breadcrumb",
    "release": "npx release-please release-pr --token=$RELEASE_PLEASE_TOKEN --repo-url=https://github.com/udayvunnam/xng-breadcrumb --release-type=node --package-name=xng-breadcrumb",
    "prepare": "husky install",
    "serve-docs": "docsify serve docs"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.2.1",
    "@angular-devkit/core": "~17.2.1",
    "@angular-devkit/schematics": "~17.2.1",
    "@angular-eslint/eslint-plugin": "~17.2.1",
    "@angular-eslint/eslint-plugin-template": "~17.2.1",
    "@angular-eslint/template-parser": "~17.2.1",
    "@angular/cli": "~17.2.1",
    "@angular/compiler-cli": "~17.2.1",
    "@angular/language-service": "~17.2.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nx/angular": "^18.0.5",
    "@nx/cypress": "18.0.5",
    "@nx/eslint-plugin": "18.0.5",
    "@nx/jest": "18.0.5",
    "@nx/js": "18.0.5",
    "@nx/linter": "18.0.5",
    "@nx/web": "18.0.5",
    "@nx/workspace": "18.0.5",
    "@schematics/angular": "~17.2.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "all-contributors-cli": "^6.25.1",
    "cypress": "^12.11.0",
    "danger": "^11.2.6",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "ng-packagr": "~17.2.0",
    "npm-run-all": "^4.1.5",
    "nx": "18.0.5",
    "nx-cloud": "latest",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@angular/animations": "~17.2.1",
    "@angular/cdk": "^17.2.1",
    "@angular/common": "~17.2.1",
    "@angular/compiler": "~17.2.1",
