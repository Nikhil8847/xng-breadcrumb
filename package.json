    "express": "^4.18.2",
    "pretty-quick": "^3.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.4"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": "please use pnpm",
    "yarn": "please use pnpm",
    "pnpm": "*"
  }
}
